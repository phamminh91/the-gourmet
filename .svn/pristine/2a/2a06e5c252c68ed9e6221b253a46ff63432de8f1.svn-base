#bannerContainer   {
	background
	padding: 7px 0 13px;
	padding-bottom:50px;
}

#banner    {
	margin: 0 auto;
	width: 972px
}

#banner .homeNav    {
	background-image: url("../img/banner_back.png");
	background-repeat: no-repeat;
	list-style-type: none;
	margin: 0;
	padding: 0;
	width: 972px;
	height: 481px
}

#banner .homeNav li {
	float: left
}

#banner .homeNav li.panel1    {
	padding: 10px 10px 68px;
	position: relative;
	width: 300px;
	height: 403px
}

#banner .homeNav li.panel1 .button a   {
	color: #999;
	font-size: 16px;
	font-family: Cochin, Georgia, "Times New Roman", Times, serif;
	font-weight: bold;
	line-height: 50px;
	text-decoration: none;
	background-image: url("../img/banner_panel1_bt_back.png");
	background-repeat: no-repeat;
	text-align: center;
	text-transform: uppercase;
	display: block;
	position: absolute;
	top: 431px;
	left: 5px;
	width: 310px;
	height: 50px
}

#banner .homeNav li.panel1 .button a:hover, #banner .homeNav li.panel1 .button a.hover   {
	color: #fff;
	background-image: url("../img/banner_panel1_bt_back_over.png");
	background-repeat: no-repeat
}


	margin:0;
	padding:0;
	text-align:center;
	font-family:Georgia;
	font-size:0.95em;
	/*background-attachment: scroll/fixed;*/
	/*background: white;	*/
	position:fixed;
	z-index:10;
	background-color: #F7F7F7;
}

.nav li{
	position:relative;
	display:inline;
}
.nav a{
	display:inline-block;
	padding:10px;
	color: #757575;
	text-decoration:none;
	font-size:1em;
}
.nav ul{
	position:absolute;
	left:-9999px;
	margin:0;
	padding:0;
	text-align:center;
}
.nav ul li{
	display:block;
}
.nav li:hover a{
	background:#f1f1f1;
	color:#DD3124;
}
.nav li:hover ul a{
	text-decoration:none;
	background:none;
}
.nav li:hover ul a:hover{
	text-decoration:none;
	background:#f1f1f1;
	/*background:#FFB097;*/
}
.nav ul a{
	white-space:nowrap;
	display:block;
	border-bottom:1px solid #ccc;

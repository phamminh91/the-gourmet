	text-decoration:none;
}

.nav{
	width:100%;
	border-bottom: 1px solid #ccc;
	box-shadow: 0px 1px 3px -2px gray;
	/*box-shadow-bottom:1px 1px 2px 2px rgba(0,0,0,.2);	*/

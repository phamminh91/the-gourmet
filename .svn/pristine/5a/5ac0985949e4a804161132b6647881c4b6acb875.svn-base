 /** Website template by freewebsitetemplates.com **/

 #page{
 	width:950px;
 	margin:0 auto;
 	padding: 0 5px;
 	font-family:'Myriad Pro';
 }

 #page .header .footer .body{
 	/*background-image:url('../images/bg-page.jpg');*/
 	width:950px;
 	overflow:hidden;
 }

 /** Start of Sidebar style **/
 #page .header .footer .body #sidebar{
 	width:211px;
 	float:left;
 }

 /** start style navitagion style**/

 #page .header .footer .body #sidebar ul.navigation{
 	margin:0 0  50px 8px;
 	padding:5px 0 0 0;
 	position:relative;
 	width:200px;
 	z-index:9990;
 }


 #page .header .footer .body #sidebar ul.navigation li{
 	width:200px;
 	height:40px;
 	position:relative;
 	line-height:40px;
 	background-image:url('../img/menu-separator.jpg');
 	background-repeat:no-repeat;
 	background-position:0 35px;

 }

 #page .header .footer .body #sidebar ul.navigation li a{
 	width:214px;
 	height:40px;
 	height:40px;
 	display:block;
 	position:absolute;
 	left:0;
 	top:-5px;
 	text-align:center;
 	padding-right:10px;
 	color:#999999;
 	text-decoration:none;
 }

 #page .header .footer .body #sidebar ul.navigation li a:hover{
 	width:214px;
 	height:40px;
 	height:40px;
 	display:block;
 	position:absolute;
 	color:#669933;
 	text-shadow: #ffffff 2px 0px 1px;
 }

 #page .header .footer .body #sidebar ul.navigation li.active a{
 	width:214px;
 	height:40px;
 	height:40px;
 	display:block;
 	position:absolute;
 	color:#669933;
 	text-shadow: #ffffff 2px 0px 1px;
 }

 #page .header .footer .body #sidebar .connect a{
 	display:block;
 	float:left;
 	text-decoration:none;
 	margin: 10px 10px 0 20px;
 }
 /** end of Sidebar style **/

 /** Start of Content Style **/

 #page .header .footer .body #content{
 	width:739px;
 	float:left;
 	padding-bottom:50px;
 	overflow:hidden;
 	padding-left:0px;
 }

 #page .header .footer .body #content .content{
 	width:726px;
 	float:left;
 	padding-top:20px;
 	padding-left:40px;
 	overflow:hidden;
 }

 /**-----------------  start table style --------------------------**/
 #page .header .footer .body #content .content table.profile-table{
 	font-size:.9em;
 	border:0;
	line-height:2em;
	width:600px;
 }
 #page .header .footer .body #content .content table.profile-table tr{
 	background: #eee; border-bottom:1px solid #036;
 }

 #page .header .footer .body #content .content table.profile-table tr.odd {
 	background:#0f0;
 }
 #page .header .footer .body #content .content table.profile-table tr.hilight {
 	background:#FFF;
 }
 #page .header .footer .body #content .content table.profile-table td{
	/*border:1px solid black;*/
	width:200px;
	padding-left:0px;
 }
 #page .header .footer .body #content .content table.profile-table td:first-child{
 	font-weight:bold;
 }

#page .header .footer .body #content .content table.profile-table td:second-child{
 	width:300px;
 }

#page .header .footer .body #content .content table.profile-table td:third-child{
 	width:100px;
}

 #page .header .footer .body #content .content table.profile-table a {
 	color: green;
 	text-decoration:underline;
 }

 #page .header .footer .body #content .content table.profile-table a:hover {
 	color: #693;
 }
 /** end table style **/